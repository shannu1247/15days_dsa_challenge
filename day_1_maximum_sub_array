from typing import List
#class Solution:
   # def maxSubArray(self, nums: List[int]) -> int:
#        summ=float('-inf')
 #       for i in range (len(nums)):
  #          curr=0
   #         for j in range(i,len(nums)):
    #            curr=curr+nums[j]
     #           summ=max(summ,curr)
      #  return summ 
class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        maxSum = float('-inf')
        currentSum = 0
        
        for num in nums:
            currentSum += num
            
            if currentSum > maxSum:
                maxSum = currentSum
            
            if currentSum < 0:
                currentSum = 0
        
        return maxSum      
sol = Solution()     
print(sol.maxSubArray([-2,1,-3,4,-1,2,1,-5,4]))